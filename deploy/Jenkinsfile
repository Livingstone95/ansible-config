 pipeline {
    agent any

  stages {
    stage('Build') {
      steps {
        script {
          sh 'echo "Building Stage"'
        }
      }
    }

    stage('Test') {
      steps {
        script {
          sh 'echo "Testing Stage"'
        }
      }
    }
   
     stage('Package') {
      steps {
        script {
          sh 'echo "Packaging app"'
        }
      }
    }

     stage('Deploy') {
      steps {
        script {
          sh 'echo "Deploying Stage"'
        }
      }
    }

     stage('Checkout') {
      steps {
        script {
          sh 'echo "Checkout"'
        }
      }
    } 
}
}

