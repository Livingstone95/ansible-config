pipeline {
    agent any

  stages {
    stage('delete workspace') {
      steps {
        dir("{$WORKSPACE}") {
          deleteDir()
        }
      }
    }
    stage('Build') {
      steps {
        script {
          sh 'echo "Building Stage"'
        }
      }
    }
    
    stage('Test') {
      steps {
        script {
          sh 'echo "Testing Stage"'
        }
      }
    }

    stage('package') {
      steps {
        script {
          sh 'echo "packaging stage"'
        }
      }
    }

    stage('deploy') {
      steps {
        script {
          sh 'echo "deploying stage"'
        }
      }
    }

    stage('clean up') {
      steps {
         cleanws()
        }
      }
    }
}


